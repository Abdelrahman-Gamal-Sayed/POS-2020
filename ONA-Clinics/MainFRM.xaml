                </StackPanel>
                <Border BorderBrush="{DynamicResource PrimaryHueMidBrush}" BorderThickness="1" HorizontalAlignment="Right" Height="46" Margin="0,86,287,0" VerticalAlignment="Top" Width="290">
                    <Canvas>
                        <StackPanel Orientation="Horizontal" Visibility="Collapsed" HorizontalAlignment="Right" VerticalAlignment="Top" FlowDirection="RightToLeft" Width="199" Canvas.Left="319">
                            <materialDesign:PackIcon Kind="Magic" Width="30" Height="30" Margin="0,0,5,0" Foreground="{DynamicResource PrimaryHueMidBrush}" VerticalAlignment="Bottom"/>
                            <ComboBox x:Name="cbx_typ_items1" materialDesign:HintAssist.Hint="نوع الحذاء "  SelectedIndex="0" Style="{StaticResource MaterialDesignFloatingHintComboBox}" Width="161" DropDownClosed="Cbx_typ_items1_DropDownClosed" >

                                <ComboBoxItem Content="رجالي"/>

                            </ComboBox>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" FlowDirection="RightToLeft"  VerticalAlignment="Top" HorizontalAlignment="Right" Width="280" Canvas.Left="8">
                            <materialDesign:PackIcon Kind="Search" Width="30" Height="30" Margin="0,0,5,0" Foreground="{DynamicResource PrimaryHueMidBrush}" VerticalAlignment="Bottom"/>

                            <TextBox x:Name="txtAName45"
                        materialDesign:HintAssist.Hint="بحث"  TextAlignment="Center"
                        Style="{StaticResource MaterialDesignFloatingHintTextBox}" Width="244" KeyDown="TxtAName45_KeyDown" />
                        </StackPanel>

                    </Canvas>

                </Border>
                <TextBlock x:Name="textBlock" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,38,635.4,0" TextWrapping="Wrap" Text="شاشة البيع النقدي وهي شاشة البيع للعملاء الذين يدفعون نقدي و هنا اختيار العميل اختياري  "  Height="56" Width="233" FlowDirection="RightToLeft" IsEnabled="False" Visibility="Collapsed"/>
            </Grid>

        </Grid>

